{
  "name": "naizo-launcher",
  "version": "1.0.0",
  "description": "Custom Minecraft Launcher for Vanilla 1.21.10 - Offline Mode",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "minecraft",
    "launcher",
    "electron"
  ],
  "author": "Naizo",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "build": {
    "appId": "com.naizo.launcher",
    "productName": "Naizo Launcher",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}